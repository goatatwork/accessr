<p id="notice"><%= notice %></p>

<h1>Switches</h1>

<table class="table table-primary">
  <thead>
    <tr>
      <th>Name</th>
      <th>Manufacturer</th>
      <th>Model</th>
      <th>Management ip</th>
      <th>Slots</th>
      <th>Ports</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @switches.each do |switch| %>
      <tr>
        <td><%= switch.name %></td>
        <td><%= switch.manufacturer %></td>
        <td><%= switch.model %></td>
        <td><%= switch.management_ip %></td>
        <td><%= switch.slots.count %></td>
        <td><%= switch.ports.count %></td>
        <td><%= link_to 'Show', switch %></td>
        <td><%= link_to 'Edit', edit_switch_path(switch) %></td>
        <td><%= link_to 'Destroy', switch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Switch', new_switch_path %>
